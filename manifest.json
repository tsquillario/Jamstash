{
	"manifest_version": 2,
	"name": "MiniSub",
	"description": "MiniSub - HTML5 Mini Player for Subsonic",
	"version": "2.3.1",
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	"icons": { 
		"16": "icon_16.png",
		"128": "icon_128.png" 
	},
	"permissions": [
		"unlimited_storage",
		"notifications",
		"http://*/*", 
		"https://*/*"
	]
}